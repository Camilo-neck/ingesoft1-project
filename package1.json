  },
  "devDependencies": {
    "@types/node": "18.7.14",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.5.0",
    "eslint": "8.23.0",
